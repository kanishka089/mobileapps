package com.diary.superdiary;

public class getsetinfologin {
    
    //private variables
    int id;
    String password;
     
    // Empty constructor
    public getsetinfologin(){
         
    }
    // constructor
    public getsetinfologin(int id, String pass){
        this.id = id;
        this.password = pass;
    }
     
    // constructor
    public getsetinfologin(String pass){
        this.password = pass;
    }
    // getting ID
    public int getID(){
        return this.id;
    }
     
    // setting id
    public void setID(int id){
        this.id = id;
    }
     
    // getting name
    public String getDate(){
        return this.date;
    }
     
    // setting name
    public void setDate(String name){
        this.date = name;
    }
     
    // getting phone number
    public String getnote(){
        return this.note;
    }
     
    // setting phone number
    public void setnote(String phone_number){
        this.note = phone_number;
    }
}
